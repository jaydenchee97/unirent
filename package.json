{
  "name": "uni_rent",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "fix:lint": "eslint ./src --fix",
    "check:ts": "tsc",
    "fix:prettier": "prettier ./src --write",
    "test": "jest",
    "test:ci": "jest --coverage --coverageReporters='json-summary' --coverageReporters='html' --forceExit"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|aws-amplify|@aws-amplify/.*)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx,tx,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js",
      "!**/.expo/**",
      "!**/tests/**",
      "/amplify/backend/function/**"
    ],
    "resolver": "./tests/common/resolver.js",
    "setupFiles": [
      "./tests/common/jestSetup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.expo",
      "/amplify/#current-cloud-backend/"
    ]
  },
  "dependencies": {
    "@aws-amplify/ui-react-native": "^1.2.24",
    "@react-google-maps/api": "^2.19.2",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-community/netinfo": "9.3.10",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "@types/react": "~18.2.14",
    "amazon-cognito-identity-js": "^6.3.2",
    "aws-amplify": "^5.3.15",
    "axios": "^1.4.0",
    "expo": "~49.0.5",
    "expo-image-picker": "~14.3.2",
    "expo-location": "~16.1.0",
    "expo-status-bar": "~1.6.0",
    "jest": "^29.2.1",
    "nanoid": "3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.4",
    "react-native": "0.72.5",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-get-random-values": "~1.9.0",
    "react-native-maps": "1.7.1",
    "react-native-paper": "^5.10.0",
    "react-native-paper-dates": "^0.18.13",
    "react-native-reanimated": "~3.15.0",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.6",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/react-native": "^12.3.0",
    "@types/jest": "^29.5.5",
    "aws-serverless-express": "^3.3.5",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.19.1",
    "cypress": "^13.3.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-universe": "^11.3.0",
    "eslint-plugin-prettier": "^5.0.0",
    "express": "^4.17.2",
    "geolib": "^3.3.4",
    "jest-expo": "^49.0.0",
    "prettier": "^3.0.1",
    "superagent": "^8.1.2",
    "supertest": "^6.3.3"
  },
  "private": true
}
